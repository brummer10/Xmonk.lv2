@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2: <http://lv2plug.in/ns/lv2core#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix guiext: <http://lv2plug.in/ns/extensions/ui#>.
@prefix time: <http://lv2plug.in/ns/ext/time#>.
@prefix units: <http://lv2plug.in/ns/extensions/units#> .
@prefix atom:  <http://lv2plug.in/ns/ext/atom#> .
@prefix urid:  <http://lv2plug.in/ns/ext/urid#> .
@prefix midi:  <http://lv2plug.in/ns/ext/midi#> .


<https://github.com/brummer10/Xmonk.lv2#me>
    a foaf:Person ;
    foaf:name "brummer" ;
    rdfs:seeAlso <https://github.com/brummer10> .

<https://github.com/brummer10/Xmonk.lv2>
    a lv2:Plugin ,
        lv2:GeneratorPlugin ;
    doap:maintainer  <https://github.com/brummer10/Xmonk.lv2#me> ;
    doap:name "Xmonk";
    doap:license <http://opensource.org/licenses/isc> ;
    lv2:requiredFeature urid:map ;
    lv2:project <https://github.com/brummer10/Xmonk.lv2> ;
    lv2:optionalFeature lv2:hardRTCapable ;

  guiext:ui <https://github.com/brummer10/Xmonk_gui>;
    
rdfs:comment """

...

""";

    lv2:port  [
        a lv2:AudioPort ,
            lv2:OutputPort ;
        lv2:index 0 ;
        lv2:symbol "out" ;
        lv2:name "Out"
    ]       , [
        a lv2:AudioPort ,
            lv2:OutputPort ;
        lv2:index 1 ;
        lv2:symbol "out1" ;
        lv2:name "Out1"
    ]       , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 2 ;
        lv2:symbol "NOTE" ;
        lv2:name "NOTE" ;
        lv2:default 40.0 ;
        lv2:minimum 28.0 ;
        lv2:maximum 52.0 ;
    ]      , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 3 ;
        lv2:symbol "GAIN" ;
        lv2:name "GAIN" ;
        lv2:default 0.25 ;
        lv2:minimum 0.0 ;
        lv2:maximum 1.0 ;
    ]      , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 4 ;
        lv2:symbol "GATE" ;
        lv2:name "GATE" ;
        lv2:default 0.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 1.0 ;
    ]      , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 5 ;
        lv2:symbol "VOWEL" ;
        lv2:name "VOWEL" ;
        lv2:default 2.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 4.0 ;
    ]      , [
        a lv2:InputPort ,
            atom:AtomPort ;
        atom:bufferType atom:Sequence ;
        atom:supports midi:MidiEvent ;
        lv2:designation lv2:control ;
        lv2:index 6 ;
        lv2:symbol "MIDI_IN" ;
        lv2:name "MIDI_IN"
    ]      , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 7 ;
        lv2:symbol "SCALE" ;
        lv2:name "SCALE" ;
        lv2:portProperty lv2:integer;
        lv2:portProperty lv2:enumeration;
        lv2:default 0.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 6.0 ;
        lv2:scalePoint [rdfs:label "---"; rdf:value 0];
        lv2:scalePoint [rdfs:label "12-TET"; rdf:value 1];
        lv2:scalePoint [rdfs:label "19-TET"; rdf:value 2];
        lv2:scalePoint [rdfs:label "24-TET"; rdf:value 3];
        lv2:scalePoint [rdfs:label "31-TET"; rdf:value 4];
        lv2:scalePoint [rdfs:label "41-TET"; rdf:value 5];
        lv2:scalePoint [rdfs:label "53-TET"; rdf:value 6];
    ]      , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 8 ;
        lv2:symbol "SUSTAIN" ;
        lv2:name "SUSTAIN" ;
        lv2:default 0.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 1.0 ;
    ]      , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:portProperty lv2:toggled;
        lv2:index 9 ;
        lv2:symbol "PANIC" ;
        lv2:name "PANIC" ;
        lv2:default 1.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 1.0 ;
    ] .

<https://github.com/brummer10/Xmonk_gui>
  a guiext:X11UI;
  guiext:binary <Xmonk_ui.so>;
        lv2:extensionData guiext::idle ; 
        lv2:requiredFeature guiext:makeResident;
  .
